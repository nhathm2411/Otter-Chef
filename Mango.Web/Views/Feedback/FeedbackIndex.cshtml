@model IEnumerable<ProductDto>

@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}

<div class="card shadow border-0">
    <div class="card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h1 class="text-white">View Feedback</h1>
                <h3 class="text-white">Product List</h3>
            </div>
        </div>
    </div>
    <div class="card-body p-4">
        <div class="row pb-3">
            <div class="col-6">
                
            </div>
        </div>
        <table id="tblProductFeedbackData" class="table table-bordered table-striped pt-3" style="width: 100%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Price</th>
                    <th>Feedback</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts {
    <script defer src="~/js/manage/productFeedback.js"></script>
}
