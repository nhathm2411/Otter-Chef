@model IEnumerable<ApplicationUser>

@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}

<div class="card shadow border-0">
    <div class="card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h1 class="text-white">Customer Account List</h1>
            </div>
        </div>
    </div>
    <div class="card-body p-4">
        <table id="tblUserData" class="table table-bordered table-striped pt-3" style="width: 100%">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Gender</th>
                    <th>Address</th>
                    <th>Birthday</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts {
    <script defer src="~/js/manage/customer.js"></script>
}
